{
  "name": "@miauflix/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run --env-file=../.env -- vite",
    "dev:ssr": "dotenvx run --env-file=../.env -- npm run build:ssr && dotenvx run --env-file=../.env -- vite preview",
    "build": "tsc --project tsconfig.app.json && npm run build:ssr",
    "build:ssr": "npm run clear && tsx build-ssr.ts",
    "clear": "rimraf ../dist/frontend/* ../dist/frontend-ssr/* node_modules/.vite node_modules/.cache .vite tsconfig.tsbuildinfo",
    "check:ts": "tsc --project tsconfig.app.json --noEmit",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "start": "npm run dev",
    "preview": "dotenvx run --env-file=../.env -- vite preview",
    "test": "vitest run --project=unit",
    "test:watch": "vitest --project=unit",
    "test:coverage": "vitest run --project=unit --coverage",
    "test:visual": "docker run --rm --ipc=host --network=host -v $PWD/..:/workspace -w /workspace/frontend -e CI=$CI mcr.microsoft.com/playwright:v1.56.1-jammy npx playwright test --project=storybook-chromium $([ \"$CI\" = \"true\" ] && echo \"--reporter=list\" || echo \"\")",
    "test:visual:update": "npm run test:visual -- --update-snapshots",
    "test:visual:ui": "playwright test --project=storybook-chromium --ui",
    "test:visual:debug": "playwright test --project=storybook-chromium --debug",
    "test:e2e": "playwright test --config=playwright.config.e2e.ts",
    "test:e2e:update": "playwright test --config=playwright.config.e2e.ts --update-snapshots",
    "test:e2e:docker": "docker run --rm --ipc=host --network=host -v $PWD/..:/workspace -w /workspace/frontend -e CI=$CI mcr.microsoft.com/playwright:v1.56.1-jammy npx playwright test --config=playwright.config.e2e.ts",
    "test:e2e:docker:update": "docker run --rm --ipc=host --network=host -v $PWD/..:/workspace -w /workspace/frontend -e CI=$CI mcr.microsoft.com/playwright:v1.56.1-jammy npx playwright test --config=playwright.config.e2e.ts --update-snapshots",
    "test:e2e:ui": "playwright test --config=playwright.config.e2e.ts --ui",
    "test:e2e:headed": "playwright test --config=playwright.config.e2e.ts --headed",
    "test:e2e:debug": "playwright test --config=playwright.config.e2e.ts --debug",
    "test:e2e:vite": "playwright test --config=playwright.config.e2e-vite.ts",
    "test:e2e:vite:update": "playwright test --config=playwright.config.e2e-vite.ts --update-snapshots",
    "test:e2e:vite:ui": "playwright test --config=playwright.config.e2e-vite.ts --ui",
    "test:e2e:vite:headed": "playwright test --config=playwright.config.e2e-vite.ts --headed",
    "test:e2e:vite:debug": "playwright test --config=playwright.config.e2e-vite.ts --debug",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@dicebear/collection": "9.2.4",
    "@dicebear/core": "9.2.4",
    "@miauflix/backend": "*",
    "@noriginmedia/norigin-spatial-navigation": "2.3.0",
    "@reduxjs/toolkit": "2.10.1",
    "framer-motion": "12.23.24",
    "gsap": "3.13.0",
    "hono": "4.10.6",
    "pluralize": "8.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-qr-code": "2.0.18",
    "react-redux": "9.2.0",
    "styled-components": "6.1.19",
    "video.js": "8.23.4",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.3",
    "@iconify/json": "2.2.408",
    "@playwright/test": "1.56.1",
    "@storybook/addon-a11y": "10.0.8",
    "@storybook/addon-vitest": "10.0.8",
    "@storybook/react-vite": "10.0.8",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jsdom": "27.0.0",
    "@types/pluralize": "0.0.33",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "@types/video.js": "7.3.58",
    "@vitejs/plugin-basic-ssl": "2.1.0",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/browser": "4.0.10",
    "@vitest/browser-playwright": "4.0.10",
    "@vitest/coverage-v8": "4.0.10",
    "eslint-plugin-storybook": "10.0.8",
    "happy-dom": "20.0.10",
    "jest-environment-jsdom": "30.2.0",
    "jsdom": "27.2.0",
    "lighthouse": "^13.0.1",
    "playwright": "1.56.1",
    "rimraf": "6.1.0",
    "sass": "1.94.1",
    "storybook": "10.0.8",
    "unplugin-icons": "22.5.0",
    "vite": "7.2.2",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vitest": "4.0.10"
  }
}
