{
  "name": "@miauflix/source-metadata-extractor",
  "version": "1.0.0",
  "description": "A library for extracting metadata from torrent information with tracker-specific enhancements",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json --resolve-full-paths",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "torrent",
    "metadata",
    "extraction",
    "tracker",
    "typescript",
    "miauflix"
  ],
  "author": "Miauflix Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/miauflix/miauflix.git",
    "directory": "packages/source-metadata-extractor"
  },
  "devDependencies": {
    "ts-jest": "^29.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0"
  }
}
